<h1>Store Name</h1>
<% if !current_user.nil? %>
  <%= link_to 'Add a Product', new_item_path %>
<% end %>

  <div>
    <% @items.each do |item| %>
      <%= image_tag(item.image_url) %> <br>
      <%= item.name %> <br>
      <%=  item.user.username %> <br>
      <%= item.description %> <br>
      <%= item.price %> <br> 
      <%= link_to "See", item %>
    <% end %>
  </div>
