<h1>Store Name</h1>
<%= link_to 'Add a Product', new_item_path %>
    <% @items.each do |item| %>
      <%= item.image_url %>
      <%= item.name %>
      <%= item.owner %>
      <%= item.description %>
      <%= item.price %>
      <%= link_to "See", item %>
      <%= link_to "Edit", edit_item_path(item) %>
          <%= link_to "Remove", item, method: :delete, data: { confirm: "Are you sure you want to delete this item?" } %>
    <% end %>
